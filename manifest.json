{
  "manifest_version": 3,
  "name": "Offlinify",
  "description": "Make websites offline",
  "version": "1.0",
  "permissions": [
    "notifications",
    "downloads",
    "scripting"
  ],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {}
}
